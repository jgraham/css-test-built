  <style type="text/css">
div
{
  width: 256px; height: 768px;
}
#outer
{
  border: 1px solid black;
}
#inner
{
  width: 256px; height: 768px;
}
#inner &gt; div { width: 256px; }
#top { background-color: lime; height: 512px; }
#bottom { background-color: aqua; height: 256px; }
  </style>
